{
  "name": "rds-js-examples",
  "version": "0.0.0",
  "description": "TypeScript/JavaScript example code using the RDS API",
  "keywords": [
    "Rich Data Services",
    "RDS",
    "MTNA",
    "Metadata",
    "Data profiling",
    "Data mapping",
    "Data transformation",
    "Data validation",
    "Data ingestion",
    "Data dissemination"
  ],
  "homepage": "https://www.richdataservices.com/",
  "contributors": [
    "Nick Richards <nick.richards@mtna.us>",
    "Will Davis <will.davis@mtna.us>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mtna/rds-js-examples"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "start": "parcel src/index.html -p 4200",
    "build": "parcel build src/index.html --no-source-maps --experimental-scope-hoisting --public-url ./",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once --pro",
    "prepush": "npm run test:prod && npm run build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.23",
    "@material/button": "^6.0.0",
    "@material/card": "^6.0.0",
    "@material/drawer": "^6.0.0",
    "@material/elevation": "^6.0.0",
    "@material/icon-button": "^6.0.0",
    "@material/layout-grid": "^6.0.0",
    "@material/select": "^6.0.0",
    "@material/tab-bar": "^6.0.0",
    "@material/top-app-bar": "^6.0.0",
    "@material/typography": "^6.0.0",
    "@rds/sdk": "^1.0.0-alpha.5",
    "@stackblitz/sdk": "^1.3.0",
    "smoothscroll-polyfill": "^0.4.4",
    "snyk": "^1.321.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^7.1.2",
    "@mtna/prettier-config-ui": "^1.0.1",
    "@types/d3-fetch": "^1.1.5",
    "@types/google.visualization": "0.0.52",
    "@types/jest": "^25.2.2",
    "@types/node": "12.12.38",
    "colors": "^1.3.2",
    "commitizen": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "d3-fetch": "^1.1.2",
    "husky": "^1.0.1",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "posthtml-include": "^1.4.3",
    "prettier": "2.0.5",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "sass": "^1.26.5",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.8.3"
  },
  "alias": {
    "shared": "./src/shared"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": "@mtna/prettier-config-ui",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "snyk": true
}
